{
   "constructor":{
      "dev":false,
      "pathsDataApps": ["www.cyberiaonline.com"],
      "generation":"0",
      "seed":null,
      "limitMbBlock":20,
      "userConfig":{
         "bridgeUrl":"https://www.cyberiaonline.com/koyn"
      }
   },
   "network":{
      "wsBridgeServer":{
         "ssl":true,
         "ws_host":"www.cyberiaonline.com/ws-koyn",
         "ws_port":3004
      },
      "structs":{
         "transactionTemplate":{
            "$id":"transaction",
            "type":"object",
            "properties":{
               "data":{
                  "$ref":"dataTransaction"
               },
               "sign":{
                  "type":"string"
               }
            },
            "required":[
               "data",
               "sign"
            ],
            "additionalProperties":false
         },
         "templateDataTransaction":{
            "$id":"dataTransaction",
            "type":"object",
            "properties":{
               "sender":{
                  "$ref":"transactionUser"
               },
               "receiver":{
                  "$ref":"transactionUser"
               },
               "amount":{
                  "type":"number"
               },
               "subject":{
                  "type":"string"
               },
               "createdDate":{
                  "type":"number"
               }
            },
            "required":[
               "sender",
               "receiver",
               "amount",
               "subject",
               "createdDate"
            ],
            "additionalProperties":false
         },
         "templateTransactionUser":{
            "$id":"transactionUser",
            "type":"object",
            "properties":{
               "data":{
                  "$ref":"publicKeySign"
               },
               "sign":{
                  "type":"string"
               }
            },
            "required":[
               "data",
               "sign"
            ],
            "additionalProperties":false
         },
         "templatePublicKeySign":{
            "$id":"publicKeySign",
            "type":"object",
            "properties":{
               "AUTH_TOKEN": {
                  "type":"string"
               },
               "base64PublicKey":{
                  "type":"string"
               },
               "B64PUKSHA256":{
                  "type":"string"
               },
               "timestamp":{
                  "type":"number"
               }
            },
            "required":[
               "base64PublicKey",
               "B64PUKSHA256",
               "timestamp"
            ],
            "additionalProperties":false
         },
         "blockMetaDataTemplate":{
            "$id":"blockMetaData",
            "type":"object",
            "properties":{
               "index":{
                  "type":"number"
               },
               "previousHash":{
                  "type":"string"
               },
               "reward":{
                  "type":"number"
               },
               "difficulty":{
                  "$ref":"difficulty"
               },
               "date":{
                  "type":"number"
               },
               "version":{
                  "type":"string"
               },
               "generation":{
                  "type":"string"
               },
               "seed":{
                  "type": ["object", "string", "null", "number", "boolean"]
               }
            },
            "required":[
               "index",
               "previousHash",
               "reward",
               "difficulty",
               "date",
               "version",
               "generation",
               "seed"
            ],
            "additionalProperties":false
         },
         "difficultyTemplate":{
            "$id":"difficulty",
            "type":"object",
            "properties":{
               "zeros":{
                  "type":"string"
               },
               "targetHash":{
                  "type":"string"
               },
               "difficulty":{
                  "type":"number"
               }
            },
            "required":[
               "zeros",
               "targetHash",
               "difficulty"
            ],
            "additionalProperties":false
         },
         "blockTemplate":{
            "$id":"block",
            "type":"object",
            "properties":{
               "node":{
                  "$ref":"node"
               },
               "block":{
                  "$ref":"blockMetaData"
               },
               "hash":{
                  "type":"string"
               },
               "nonce":{
                  "type":"number"
               }
            },
            "required":[
               "node",
               "block",
               "hash",
               "nonce"
            ],
            "additionalProperties":false
         },
         "nodeTemplate":{
            "$id":"node",
            "type":"object",
            "properties":{
               "dataTransaction":{
                  "type":"array",
                  "items":{
                     "$ref":"transaction"
                  }
               },
               "dataApp":{
                  "type":"array",
                  "items":{
                     "$ref":"dataApp"
                  }
               },
               "rewardAddress":{
                  "type":"string"
               }
            },
            "required":[
               "dataTransaction",
               "dataApp",
               "rewardAddress"
            ],
            "additionalProperties":false
         },
         "dataAppTemplate":{
            "$id":"dataApp",
            "type":"object",
            "properties":{
               "path":{
                  "type":"string"
               },
               "data":{
                  "$ref":"timeFlowObjs"
               },
               "sign":{
                  "type":"string"
               }
            },
            "required":[
               "path",
               "data",
               "sign"
            ],
            "additionalProperties":false
         },
         "timeFlowObjsTemplate":{
            "$id":"timeFlowObjs",
            "type":"object",
            "properties":{
               "emit":{
                  "type":"array",
                  "items":{
                     "type":"object"
                  }
               },
               "transactions":{
                  "type":"array",
                  "items":{
                     "type":"object"
                  }
               },
               "timestamp":{
                  "type":"number"
               }
            },
            "required":[
               "emit",
               "transactions",
               "timestamp"
            ],
            "additionalProperties":false
         }
      }
   }
